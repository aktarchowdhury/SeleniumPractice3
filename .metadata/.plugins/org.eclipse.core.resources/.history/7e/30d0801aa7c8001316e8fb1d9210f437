package com.infix.tech;

import org.junit.Before;
import org.junit.Test;

import com.thoughtworks.selenium.DefaultSelenium;

public class SeleniumRC {
	DefaultSelenium selenium=new DefaultSelenium("localhost", 4444, "*firefox", "http://www.facebook.com");
	@Before
public void BrowserOpen(){
		selenium.start();
		selenium.open("/");
		selenium.windowMaximize();
		selenium.setSpeed("4000");
	
}
	@Test
	public void LogIn(){
		selenium.type("email", "aktarchowdhury7@gmail.com");
		selenium.type("pass", "aa786786");
		selenium.click("u_0_n");
	}
}
